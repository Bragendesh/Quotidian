            <string>//Command List for reference.
// 0 = No Command, DO NOT GENERATE THIS.
// 1 = Brush teeth
// 2 = Fix hair
// 3 = Knock on Mirror
// 4 = Apply lipstick
// 5 = Shave face
// 6 = Turn sink on and off 
// Will be added
var numberOfCommands = 6; //Number of commands is equal to the highest command assignment.

///Generate Commands.
global.numberOfTasks = global.difficulty;
global.winner = false;
var hairExists = false;
var lipstickExists = false;
var beardExists = false;

var i;
for (i = 0; i &lt; global.numberOfTasks; i++){
   var temp = irandom(numberOfCommands-1) + 1;
   //Check for duplicate events that can't be duplicated.
   while ((temp == 2 &amp;&amp; hairExists) || (temp == 4 &amp;&amp; lipstickExists) || (temp == 5 &amp;&amp; beardExists)) {
        temp = irandom(numberOfCommands-1) + 1;
   }
   //Set flags.
   if (temp == 2) {
        hairExists = true;
   } else if (temp == 4) {
        lipstickExists = true;
   } else if (temp == 5) {
        beardExists = true;
   }
   global.commandList[i] = temp;
}
</string>
